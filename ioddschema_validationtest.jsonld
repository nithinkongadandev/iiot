 {
    "@context": [
        "https://www.w3.org/2022/wot/td/v1.1",
        {
            "time": "http://www.w3.org/2006/time#"
        }
    ],
    "@type": "tm:ThingModel",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "attributes": {
            "type": "object",
            "properties": {
                "documentInfo": {
                    "type": "object",
